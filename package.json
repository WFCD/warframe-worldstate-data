{
  "name": "warframe-worldstate-data",
  "version": "1.0.5",
  "description": "Warframe data for use with warframe-worldstate-parser",
  "main": "exports.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-json": "^1.0.0",
    "chai-json-schema": "^1.5.1",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "precommit-hook": "^3.0.0",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "test": "nyc mocha",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "sort": "python tools/sort_languages.py",
    "validate": "npm run lint:fix && npm run test && git add -u ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wfcd/warframe-worldstate-data.git"
  },
  "keywords": [
    "warframe-worldstate",
    "javascript",
    "warframe"
  ],
  "author": "tobitenno",
  "contributors": [
    "tobitenno",
    "cagatayldzz",
    "scruffyrules",
    "nspacestd",
    "dsinn",
    "msaracevic",
    "Markcreator",
    "delfunua",
    "esunder",
    "dbarbuzzi",
    "ViriatoLusitano",
    "iPeer",
    "IhToN",
    "OrcusCZ",
    "DaxterSpeed",
    "sgkoishi",
    "Hikoaki",
    "Tithen-Firion"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wfcd/warframe-worldstate-data/issues"
  },
  "homepage": "https://github.com/wfcd/warframe-worldstate-data#readme",
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "eslintIgnore": [],
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false,
          "preferType": {
            "String": "string",
            "Number": "number",
            "Boolean": "boolean",
            "Function": "function",
            "object": "Object",
            "date": "Date",
            "error": "Error"
          },
          "prefer": {
            "return": "returns"
          }
        }
      ],
      "strict": [
        "error",
        "safe"
      ],
      "linebreak-style": "off",
      "no-restricted-syntax": [
        "off"
      ],
      "no-await-in-loop": "off",
      "import/no-unresolved": 0
    }
  },
  "nyc": {
    "exclude": [
      "test/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  },
  "mocha": {},
  "pre-commit": [
    "validate"
  ]
}
